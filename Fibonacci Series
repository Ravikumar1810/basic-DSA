public class Fibonacci {
    public static void main(String[] args) {
        int n = 10, num1 = 0, num2 = 1;
        System.out.println("Fibbonacci Series:" +num1+ "," +num2);
    for(int i = 2; i<= n; i++) {
        int fib = num1 + num2;
        System.out.print("," +fib);
        num1 = num2;
        num2 = fib;
    }
    }
}
